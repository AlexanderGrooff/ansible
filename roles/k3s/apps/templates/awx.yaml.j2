---
apiVersion: awx.ansible.com/v1beta1
kind: AWX
metadata:
  name: awx
  namespace: awx
spec:
  service_type: ClusterIP
  replicas: 1

  # Ingress settings
  ingress_type: ingress
  hostname: awx.agrooff.com
  ingress_annotations: |
    kubernetes.io/ingress.class: "nginx"
  ingress_tls_secret: awx-tls-secret
